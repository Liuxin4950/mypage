{
    "manifest_version": 3,
    "name": "My iTab",
    "version": "1.0",
    "description": "A custom new tab extension.",
    "permissions": [
        "storage"
    ],
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "assets/icon16.png",
            "48": "assets/icon48.png",
            "128": "assets/icon128.png"
        }
    },
    "icons": {
        "16": "assets/icon16.png",
        "48": "assets/icon48.png",
        "128": "assets/icon128.png"
    }
}